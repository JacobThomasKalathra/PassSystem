#TIP
{
    position:absolute; 
    width: 100px; 
    top: 0px; 
    left: 0px; 
    z-index:4; 
    visibility:hidden;
}
#COUNT
{
    position:absolute; 
    width: 100px; 
    top: 0px; 
    left: 0px; 
    z-index:4; 
    visibility:hidden;
}
#ERROR
{
    position:absolute; 
    width: 100px; 
    top: 0px; 
    left: 0px; 
    z-index:5; 
    visibility:hidden;
}

